package  {	import com.google.maps.Map;	import com.google.maps.MapEvent;	import flash.display.Sprite;	import flash.net.XMLSocket;	import flash.events.DataEvent;	import flash.geom.Point;	public class Gps extends Sprite	{		public static const PORT:Number = 5331;			public static const API_KEY:String = "";		public static const LOCALHOST:String = "127.0.0.1";				private var map:Map = null;		private var socket:XMLSocket = null;				public function Gps() 		{			super();			init();		}				private function init():void		{			map = new Map();			map.key = API_KEY;			map.setSize( new Point( 550, 400 ) );			map.addEventListener( MapEvent.MAP_READY, doMapReady );			addChildAt( map, 0 );						socket = new XMLSocket();			socket.addEventListener(DataEvent.DATA, doSocketData );			socket.connect( LOCALHOST, PORT );		}				protected function doMapReady( event:MapEvent ):void		{					}				protected function doSocketData( event:DataEvent ):void		{			trace( event.data.toString() );		}	}}